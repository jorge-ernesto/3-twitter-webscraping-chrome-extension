{
  "manifest_version": 3,
  "name": "Twitter WebScraping Chrome Extension",
  "version": "1.0",
  "description": "Esta es la descripción de mi extensión",
  "icons": {
    "128": "assets/img/logo1.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}